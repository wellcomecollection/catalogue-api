          },
          "pattern_replace_uu_w": {
            "pattern": "uu",
            "type": "pattern_replace",
            "replacement": "w"
          },
          "pattern_replace_vv_w": {
            "pattern": "vv",
            "type": "pattern_replace",
            "replacement": "w"
          },
          "hindi_stemmer": {
            "type": "stemmer",
            "language": "hindi"
          },
