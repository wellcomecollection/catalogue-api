              "l",
              "all",
              "dall",
              "dell",
              "nell",
              "sull",
              "coll",
              "pell",
              "gl",
              "agl",
              "dagl",
              "degl",
              "negl",
              "sugl",
              "un",
              "m",
              "t",
              "s",
              "v",
              "d"
            ],
